background = circle(150, "solid", "lightgrey")
eye1 = circle(30, "solid", "red")
eye2 = circle(30, "solid", "red")
nose = rectangle(30, 30, "solid", "black")
mouth = rectangle(150, 15, "solid", "black")


face =
  put-image(eye1, 100, 190,
    put-image(eye2, 200, 190,
      put-image(nose, 150, 120,
        put-image(mouth, 150, 67,
          
          background))))
face
